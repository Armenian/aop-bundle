{
    "name": "dmp/aop-bundle",
    "description": "Adds AOP capabilities to Symfony5",
    "keywords": ["annotations","aop"],
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        },
        {
            "name": "David Akopyan",
            "email": "david_akopyan@yahoo.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "dmp/cg-lib": "^0.1"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpunit/phpunit": "^9.5",
        "symfony/http-kernel": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "DMP\\AopBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMP\\AopBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-cc": "phpunit coverage-html report/"
    }
}
